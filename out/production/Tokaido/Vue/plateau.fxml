<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<TabPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="0.0" minWidth="0.0" prefHeight="550.0" prefWidth="1600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.OptionController">
  <tabs>
    <Tab text="Partie">
      <content>
        <AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="550.0" prefWidth="1500.0" styleClass="background" stylesheets="@style.css" />
      </content>
    </Tab>
    <Tab onSelectionChanged="#changeTailleEcranOption" text="Options">
      <content>
        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="550.0" prefWidth="1600.0" styleClass="tabOption" stylesheets="@style.css">
               <children>
                  <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="157.0" styleClass="buttonClassique" stylesheets="@style.css" text="Reccomencer" />
                  <Button layoutX="14.0" layoutY="109.0" mnemonicParsing="false" onAction="#leaveGame" prefHeight="26.0" prefWidth="158.0" styleClass="buttonQuitter" stylesheets="@style.css" text="Quitter" />
               </children></AnchorPane>
      </content>
    </Tab>
  </tabs>
</TabPane>
